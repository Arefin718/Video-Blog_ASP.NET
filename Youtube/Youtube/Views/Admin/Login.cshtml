@model YoutubeEntity.Class.Admin

@{
    ViewBag.Title = "Login";
  
}

<h2>Login</h2>
<!DOCTYPE html>
<html>


<!-- Mirrored from nilethemes.com/html/video-post/07-log_in_page.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 10 Dec 2017 17:36:51 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Video Post – Video Sharing HTML Template</title>
    <meta name="keywords" content="Blog website templates" />
    <meta name="description" content="Author - Personal Blog Wordpress Template">
    <meta name="author" content="Rabie Elkheir">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Bootstrap Core CSS -->
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" />
    <!-- Owl Carousel Assets -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />
    <!--Google Fonts-->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800|Raleway:400,500,700|Roboto:300,400,500,700,900|Ubuntu:300,300i,400,400i,500,500i,700" rel="stylesheet">
    <!-- Main CSS -->
    <link rel="stylesheet" href="~/Content/css/style.css" />
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="~/Content/css/responsive.css" />
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>

</head>

<body class="log_in_page">
    <!--======= log_in_page =======-->
    <div id="log-in" class="site-form log-in-form">

        <div id="log-in-head">
            <h1>Log in</h1>
            <div id="logo"><a href="01-home.html"><img src="img/logo.png" alt=""></a></div>
        </div>

        <div class="form-output">
            <form method="post">
                <div class="form-group label-floating">
                    <label class="control-label">Your Email</label>
                    <input class="form-control" name="Email" placeholder="" type="email">
                </div>
                <div class="form-group label-floating">
                    <label class="control-label">Your Password</label>
                    <input class="form-control" name="Password" placeholder="" type="password">
                </div>

                <div class="form-group label-floating">
                    <label class="control-label h5 text-danger">@TempData["ErrorMessage"] </label>
                </div>


                <div class="remember">
                    <div class="checkbox">
                        <label>
                            <input name="optionsCheckboxes" type="checkbox">
                            Remember Me
                        </label>
                    </div>
                    <a href="#" class="forgot">Forgot my Password</a>
                </div>

                <button type="submit" class="btn btn-lg btn-primary full-width">Login</button>

                <div class="or"></div>

                <a href="#" class="btn btn-lg bg-facebook full-width btn-icon-left"><i class="fa fa-facebook" aria-hidden="true"></i>Login with Facebook</a>

                <a href="#" class="btn btn-lg bg-twitter full-width btn-icon-left"><i class="fa fa-twitter" aria-hidden="true"></i>Login with Twitter</a>


                <p>Don't you have an account? <a href="/user/registration">Register Now!</a> </p>
            </form>
        </div>
    </div>
    <!--======= // log_in_page =======-->
</body>



<!-- Mirrored from nilethemes.com/html/video-post/07-log_in_page.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 10 Dec 2017 17:36:51 GMT -->
</html>
