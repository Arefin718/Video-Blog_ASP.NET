@model YoutubeEntity.Class.Channel

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Navigation.cshtml";
}

<div class="container">
    <div class="col-md-12">

        <div class="row">
            <div class="col-md-12 text-center">
                <h1 class="text-white">Channel Details</h1>
            </div>
        </div>

        <div>
   
            <hr />
            <dl class="dl-horizontal">

                <dt>
                   Channel Name
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ChannelName)
                </dd>

                <dt>
                    Channel Details
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ChannelDetails)
                </dd>

                <dt>
                    Channel Type
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ChannelType)
                </dd>

                <dt>
                    Total Subscriber
                </dt>

                <dd>
                    @*@Html.DisplayFor(model => model.TotalSubscriber)*@
                </dd>

            </dl>
        </div>

    </div>
</div>
